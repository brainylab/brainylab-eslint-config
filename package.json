{
  "name": "@brainylab/eslint-config",
  "version": "2.0.0",
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "AndrÃ© Felipe Schulle",
    "email": "dev@brainylab.com.br"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brainylab/eslint-config.git"
  },
  "exports": {
    "import": "./dist/configs/index.js",
    "require": "./dist/configs/index.cjs"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "keywords": [
    "eslint",
    "config",
    "preset",
    "brainylab"
  ],
  "description": "ESLint config for BrainyLab projects",
  "main": "index.js",
  "scripts": {
    "build": "pnpm tsup && cp ./legacy/* dist",
    "commit:add": "git add .",
    "commit:detail": "cz",
    "commit:push": "git push",
    "commit": "pnpm commit:add && pnpm commit:detail && pnpm commit:push"
  },
  "dependencies": {
    "@eslint/js": "9.4.0",
    "@react-native/eslint-plugin": "0.74.0",
    "@typescript-eslint/eslint-plugin": "6.19.1",
    "@typescript-eslint/parser": "6.19.1",
    "eslint": "9.4.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-define-config": "2.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-import-helpers": "1.3.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-native": "4.1.0",
    "globals": "15.4.0",
    "typescript-eslint": "7.13.0"
  },
  "devDependencies": {
    "@brainylab/prettier-config": "0.1.4",
    "@types/eslint-config-prettier": "6.11.3",
    "@types/eslint__js": "8.42.3",
    "@types/node": "20.14.2",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint-plugin-import-x": "0.5.1",
    "tsup": "8.1.0",
    "tsx": "4.15.2",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "eslint": ">=8",
    "typescript": ">=4"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
