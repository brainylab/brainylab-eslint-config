{
	"name": "@brainylab/eslint-config-node",
	"version": "3.0.0",
	"description": "ESLint node config",
	"keywords": [
		"brainylab",
		"eslint",
		"config",
		"node"
	],
	"license": "MIT",
	"author": "Andr√© Felipe Schulle <andrefelipeschulle@icloud.com>",
	"type": "module",
	"main": "./src/index.ts",
