		"@brainylab/eslint-config-typescript": "workspace:*",
		"eslint": "9.15.0",
		"eslint-define-config": "2.1.0",
		"eslint-plugin-n": "17.14.0",
		"globals": "15.12.0"
	},
	"devDependencies": {
		"clean-package": "2.2.0"
	},
	"clean-package": "../clean-package.config.json"
}
